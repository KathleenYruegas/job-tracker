<h1>Dashboard#index</h1>
<p>Find me in app/views/dashboard/index.html.erb</p>

<div class="job_count">
  <ul>
    <% @city_counts.each do |city, count| %>
    <li>
      <%= link_to "#{city} Jobs", jobs_path(:location => city) %>
      <%= "#{count} #{'job'.pluralize(count)}" %>
    </li>
    <% end %>
  </ul>
</div>
